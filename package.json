{
  "name": "management-dashboard",
  "version": "1.0.0",
  "description": "test 2 of web design course",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass assests/sass/main.scss assests/css/style.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "node-sass assests/sass/main.scss assests/css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' assests/css/style.comp.css -o assests/css/style.prefix.css",
    "compress:css": "node-sass assests/css/style.prefix.css assests/css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/divyang-softrefine/test-2.git"
  },
  "keywords": [
    "first"
  ],
  "author": "Divyang",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/divyang-softrefine/test-2/issues"
  },
  "homepage": "https://github.com/divyang-softrefine/test-2#readme",
  "dependencies": {
    "autoprefixer": "^7.1.4",
    "concat": "^1.0.3",
    "node-sass": "^7.0.1",
    "npm-run-all": "^4.1.1",
    "postcss-cli": "^4.1.1"
  }
}
